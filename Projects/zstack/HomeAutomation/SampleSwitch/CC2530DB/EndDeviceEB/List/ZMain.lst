    296           *
    297           * @brief       This displays the IEEE (MSB to LSB) on the LCD.
    298           *
    299           * input parameters
    300           *
    301           * None.
    302           *
    303           * output parameters
    304           *
    305           * None.
    306           *
    307           * @return      None.
    308           **************************************************************************************************
    309           */
    310          static void zmain_dev_info(void)
    311          {
    312          #if defined ( SERIAL_DEBUG_SUPPORTED ) || (defined ( LEGACY_LCD_DEBUG ) && defined (LCD_SUPPORTED))
    313            uint8 i;
    314            uint8 *xad;
    315            uint8 lcd_buf[Z_EXTADDR_LEN*2+1];
    316          
    317            // Display the extended address.
    318            xad = aExtendedAddress + Z_EXTADDR_LEN - 1;
    319          
    320            for (i = 0; i < Z_EXTADDR_LEN*2; xad--)
    321            {
    322              uint8 ch;
    323              ch = (*xad >> 4) & 0x0F;
    324              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    325              ch = *xad & 0x0F;
    326              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    327            }
    328            lcd_buf[Z_EXTADDR_LEN*2] = '\0';
    329            HalLcdWriteString( "IEEE: ", HAL_LCD_DEBUG_LINE_1 );
    330            HalLcdWriteString( (char*)lcd_buf, HAL_LCD_DEBUG_LINE_2 );
    331          #endif
    332          }
