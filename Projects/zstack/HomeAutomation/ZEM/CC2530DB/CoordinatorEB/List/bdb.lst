   1959                }
   1960                else
   1961                {
   1962                  prev_desc->nextDesc = current_desc->nextDesc;
   1963                }
   1964                
   1965                osal_mem_free( current_desc );
   1966                
   1967                return ZSuccess;
   1968              }
   1969          
   1970              prev_desc = current_desc;
   1971              current_desc = current_desc->nextDesc;
