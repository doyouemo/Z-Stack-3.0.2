    254          /**************************************************************************************************
    255           * @fn          zmain_dev_info
    256           *
    257           * @brief       This displays the IEEE (MSB to LSB) on the LCD.
    258           *
    259           * input parameters
    260           *
    261           * None.
    262           *
    263           * output parameters
    264           *
    265           * None.
    266           *
    267           * @return      None.
    268           **************************************************************************************************
    269           */
    270          static void zmain_dev_info(void)
    271          {
    272          #if defined ( SERIAL_DEBUG_SUPPORTED ) || (defined ( LEGACY_LCD_DEBUG ) && defined (LCD_SUPPORTED))
    273            uint8 i;
    274            uint8 *xad;
    275            uint8 lcd_buf[Z_EXTADDR_LEN*2+1];
    276          
    277            // Display the extended address.
    278            xad = aExtendedAddress + Z_EXTADDR_LEN - 1;
    279          
    280            for (i = 0; i < Z_EXTADDR_LEN*2; xad--)
    281            {
    282              uint8 ch;
    283              ch = (*xad >> 4) & 0x0F;
    284              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    285              ch = *xad & 0x0F;
    286              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    287            }
    288            lcd_buf[Z_EXTADDR_LEN*2] = '\0';
    289            HalLcdWriteString( "IEEE: ", HAL_LCD_DEBUG_LINE_1 );
    290            HalLcdWriteString( (char*)lcd_buf, HAL_LCD_DEBUG_LINE_2 );
    291          #endif
    292          }
